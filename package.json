{
  "name": "paul-harris",
  "version": "1.0.0",
  "description": "Paul-Harris-app",
  "engines": {
    "node": "10.16.3"
  },
  "main": "src/app.ts",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node  --transpile-only ./src/app.ts --max_old_space_size=2048",
    "start": "ts-node  --transpile-only ./src/app.ts",
    "start:prod": "npm run build:prod && pm2 start ecosystem.config.js --env production",
    "inspect": "nodemon --inspect src/app.ts",
    "test": "jest",
    "lint": "npm run lint:js ",
    "lint:eslint": "eslint --ignore-path .gitignore --ext .ts",
    "lint:js": "npm run lint:eslint src/",
    "lint:fix": "npm run lint:js -- --fix",
    "copy-static": "copyfiles -E -u 1 -e \"**/*.ts\" \"src/db/sql/sql/**\" \"build/\"",
    "production": "set NODE_ENV=production && node build/app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jdn000/phapp.git"
  },
  "keywords": [
    "education",
    "js",
    "javascript",
    "typescript",
    "node",
    "express"
  ],
  "author": "Julio Diaz N",
  "license": "ISC",
  "dependencies": {
    "agenda": "^2.0.2",
    "agendash": "^1.0.0",
    "argon2": "^0.19.1",
    "bluebird": "^3.7.2",
    "body-parser": "^1.18.2",
    "celebrate": "^9.1.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.0",
    "cors": "^2.8.4",
    "dotenv": "^5.0.1",
    "errorhandler": "^1.5.0",
    "event-dispatch": "^0.4.1",
    "eventemitter3": "^3.1.0",
    "express": "^4.16.2",
    "express-basic-auth": "^1.2.0",
    "express-jwt": "^5.3.1",
    "jsonwebtoken": "^8.2.0",
    "lodash": "^4.17.13",
    "mailgun-js": "^0.22.0",
    "method-override": "^3.0.0",
    "moment": "^2.29.0",
    "moment-timezone": "^0.5.23",
    "mongoose": "^5.4.13",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "pdfkit": "^0.11.0",
    "pg-monitor": "^1.3.1",
    "pg-promise": "^10.3.5",
