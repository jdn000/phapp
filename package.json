    "@types/jest": "^23.3.8",
    "@types/lodash": "^4.14.118",
    "@types/mongoose": "^5.3.17",
    "@types/multer": "^1.3.10",
    "@types/node": "^10.14.8",
    "@types/pg": "^7.14.1",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "@typescript-eslint/parser": "^1.7.0",
    "copyfiles": "^2.4.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-prettier": "^3.0.1",
    "jest": "^24.1.0",
    "nodemon": "^2.0.1",
